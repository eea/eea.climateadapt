<?xml version="1.0"?>

<portlet-app
	version="2.0"
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
>
	
	<portlet>
		<portlet-name>measureportlet</portlet-name>
		<display-name>MeasurePortlet</display-name>
		<portlet-class>
			nl.wur.alterra.cgi.ace.portlet.MeasurePortlet
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/measure/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/measure/edit.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<!-- Resource Bundle -->
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>MeasurePortlet</title>
			<short-title>MeasurePortlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<!-- Portlet preferences (configurable by user in portlet EDIT mode) -->
		<portlet-preferences>
			<preference>
				<name>orderByCol</name>
				<value>name</value>
			</preference>
			<preference>
				<name>orderByType</name>
				<value>asc</value>
			</preference>
		</portlet-preferences>		
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>backportlet</portlet-name>
		<display-name>BackPortlet</display-name>
		<portlet-class>
			nl.wur.alterra.cgi.ace.portlet.BackPortlet
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/viewback.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<!-- Resource Bundle -->
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>BackPortlet</title>
			<short-title>BackPortlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>measuredetailportlet</portlet-name>
		<display-name>MeasureDetailPortlet</display-name>
		<portlet-class>
			nl.wur.alterra.cgi.ace.portlet.MeasureDetailPortlet
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/measuredetail/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<!-- Resource Bundle -->
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>MeasureDetailPortlet</title>
			<short-title>MeasureDetailPortlet</short-title>
			<keywords></keywords>
		</portlet-info>	
		<!-- Portlet preferences (configurable by user in portlet EDIT mode) -->
		<portlet-preferences>
			<preference>
				<name>editUrl</name>
				<value>/web/guest/measures</value>
			</preference>
		</portlet-preferences>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>measuregoeditportlet</portlet-name>
		<display-name>MeasureGoEditPortlet</display-name>
		<portlet-class>
			nl.wur.alterra.cgi.ace.portlet.MeasureGoEditPortlet
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/measuregoedit/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/measuregoedit/edit.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
		<portlet-info>
			<title>MeasureGoEditPortlet</title>
			<short-title>MeasureGoEditPortlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>