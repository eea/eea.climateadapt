<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_0_0.dtd">
<service-builder package-path="nl.wur.alterra.cgi.ace">
	<author>groot052</author>
	<namespace>Ace</namespace>
	<!-- Define the entity for the Ace_Measure table -->
	<entity name="Measure" local-service="true" remote-service="false">

		<!-- Primary key column -->
		<column name="measureId" type="long" primary="true" />

		<!-- Foreign key columns -->
		<column name="companyId" type="long" />
		<column name="groupId" type="long" />

		<!-- Other columns -->
		<column name="name" type="String" />
		<column name="description" type="String" />
		<column name="implementationtype" type="String" />
		<column name="implementationtime" type="String" />
		<column name="lifetime" type="String" />
		<column name="spatiallayer" type="String" />
		<column name="spatialvalues" type="String" />
		<column name="legalaspects" type="String" />
		<column name="stakeholderparticipation" type="String" />
		<column name="contact" type="String" />
		<column name="succeslimitations" type="String" />
		<column name="website" type="String" />
		<column name="costbenefit" type="String" />
		<column name="keywords" type="String" />
		<column name="startdate" type="Date" />
		<column name="enddate" type="Date" />
		<column name="publicationdate" type="Date" />
		<column name="language" type="String" />
		<column name="sectors_" type="String" />
		<column name="elements_" type="String" />
		<column name="climateimpacts_" type="String" />
		<column name="mao_type" type="String" />

		<!-- Sort order -->
		<order by="asc" >
			<order-column name="name" />
		</order>

		<!-- Finder Methods -->
		<finder name="GroupId" return-type="Collection">
			<finder-column name="groupId" />
		</finder>

	</entity>
</service-builder>
