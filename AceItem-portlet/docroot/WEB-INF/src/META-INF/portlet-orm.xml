<?xml version="1.0" encoding="UTF-8"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="nl.wur.alterra.cgi.ace.model.impl.AceSectorModelImpl">
		<attributes>
			<id name="sectorId" />
			<basic name="sector" />
			<transient name="primaryKey" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="nl.wur.alterra.cgi.ace.model.impl.AceElementModelImpl">
		<attributes>
			<id name="elementId" />
			<basic name="element" />
			<transient name="primaryKey" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="nl.wur.alterra.cgi.ace.model.impl.AceClimateImpactModelImpl">
		<attributes>
			<id name="impactId" />
			<basic name="impact" />
			<transient name="primaryKey" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="nl.wur.alterra.cgi.ace.model.impl.AceItemModelImpl">
		<attributes>
			<id name="aceItemId" />
			<basic name="companyId" />
			<basic name="groupId" />
			<basic name="name" />
			<basic name="description" />
			<basic name="datatype" />
			<basic name="storedAt" />
			<basic name="storagetype" />
			<basic name="language" />
			<basic name="textSearch" />
			<basic name="keyword" />
			<basic name="targetresolution" />
			<basic name="spatialLayer" />
			<basic name="spatialValues" />
			<basic name="startDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="endDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="publicationDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="sectors_" />
			<basic name="elements_" />
			<basic name="climateimpacts_" />
			<transient name="primaryKey" />
		</attributes>
	</mapped-superclass>
	<entity class="nl.wur.alterra.cgi.ace.model.impl.AceSectorImpl" name="AceSector">
		<table name="Ace_AceSector" />
		<attributes />
	</entity>
	<entity class="nl.wur.alterra.cgi.ace.model.impl.AceElementImpl" name="AceElement">
		<table name="Ace_AceElement" />
		<attributes />
	</entity>
	<entity class="nl.wur.alterra.cgi.ace.model.impl.AceClimateImpactImpl" name="AceClimateImpact">
		<table name="Ace_AceClimateImpact" />
		<attributes />
	</entity>
	<entity class="nl.wur.alterra.cgi.ace.model.impl.AceItemImpl" name="AceItem">
		<table name="Ace_AceItem" />
		<attributes />
	</entity>
</entity-mappings>