<?xml version="1.0" encoding="UTF-8"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">

	<portlet>
		<portlet-name>aceitemportlet</portlet-name>
		<display-name>AceItem</display-name>
		<portlet-class>nl.wur.alterra.cgi.ace.portlet.AceItemPortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/aceitem/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/aceitem/edit.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
			<portlet-mode>EDIT</portlet-mode>
		</supports>
        <!-- Resource Bundle -->
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>AceItem</title>
			<short-title>AceItemPortlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<!-- Portlet preferences (configurable by user in portlet EDIT mode) -->
		<portlet-preferences>
			<preference>
				<name>rowsPerPage</name>
				<value>75</value>
			</preference>
			<preference>
				<name>orderByCol</name>
				<value>name</value>
			</preference>
			<preference>
				<name>orderByType</name>
				<value>asc</value>
			</preference>			
		</portlet-preferences>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>wxsharvesterportlet</portlet-name>
		<display-name>WxS Harvesters</display-name>
		<portlet-class>nl.wur.alterra.cgi.ace.portlet.WxsHarvesterPortlet</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/wxsharvester/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>VIEW</portlet-mode>
		</supports>
		<!-- Resource Bundle -->
		<resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>WxS Harvester</title>
			<short-title>WxSHarvesterPortlet</short-title>
			<keywords></keywords>
		</portlet-info>
		<!-- Portlet preferences (configurable by user in portlet EDIT mode) -->
		<portlet-preferences>
			<preference>
				<name>rowsPerPage</name>
				<value>10</value>
			</preference>
		</portlet-preferences>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>

    <portlet>
    	<portlet-name>aceitemdetailportlet</portlet-name>
    	<display-name>AceItemDetailPortlet</display-name>
    	<portlet-class>
    		nl.wur.alterra.cgi.ace.portlet.AceItemDetailPortlet
    	</portlet-class>
    	<init-param>
    		<name>view-jsp</name>
    		<value>/html/aceitemdetail/view.jsp</value>
    	</init-param>
    	<expiration-cache>0</expiration-cache>
    	<supports>
    		<mime-type>text/html</mime-type>
    		<portlet-mode>VIEW</portlet-mode>
    	</supports>
		<!-- Resource Bundle -->
		<resource-bundle>content.Language</resource-bundle>
    	<portlet-info>
    		<title>AceItemDetailPortlet</title>
    		<short-title>AceItemDetailPortlet</short-title>
    		<keywords></keywords>
    	</portlet-info>
    	<portlet-preferences>
			<preference>
				<name>editUrl</name>
				<value>/web/guest/aceitems1</value>
			</preference>
		</portlet-preferences>
    	<security-role-ref>
    		<role-name>administrator</role-name>
    	</security-role-ref>
    	<security-role-ref>
    		<role-name>guest</role-name>
    	</security-role-ref>
    	<security-role-ref>
    		<role-name>power-user</role-name>
    	</security-role-ref>
    	<security-role-ref>
    		<role-name>user</role-name>
    	</security-role-ref>
    </portlet>
    <portlet>
    	<portlet-name>aceitemgoeditportlet</portlet-name>
    	<display-name>AceItemGoEditPortlet</display-name>
    	<portlet-class>
    		nl.wur.alterra.cgi.ace.portlet.AceItemGoEditPortlet
    	</portlet-class>
    	<init-param>
    		<name>view-jsp</name>
    		<value>/html/aceitemgoedit/view.jsp</value>
    	</init-param>
    	<init-param>
    		<name>edit-jsp</name>
    		<value>/html/aceitemgoedit/edit.jsp</value>
    	</init-param>
    	<expiration-cache>0</expiration-cache>
    	<supports>
    		<mime-type>text/html</mime-type>
    		<portlet-mode>VIEW</portlet-mode>
    		<portlet-mode>EDIT</portlet-mode>
    	</supports>
        <!-- Resource Bundle -->
		<resource-bundle>content.Language</resource-bundle>
    	<portlet-info>
    		<title>AceItemGoEditPortlet</title>
    		<short-title>AceItemGoEditPortlet</short-title>
    		<keywords></keywords>
    	</portlet-info>
    	<!-- Portlet preferences (configurable by user in portlet EDIT mode) -->
		<portlet-preferences>
			<preference>
				<name>editUrl</name>
				<value>/web/guest/aceitems1</value>
			</preference>
		</portlet-preferences>
    	<security-role-ref>
    		<role-name>administrator</role-name>
    	</security-role-ref>
    	<security-role-ref>
    		<role-name>guest</role-name>
    	</security-role-ref>
    	<security-role-ref>
    		<role-name>power-user</role-name>
    	</security-role-ref>
    	<security-role-ref>
    		<role-name>user</role-name>
    	</security-role-ref>
    </portlet>
</portlet-app>
