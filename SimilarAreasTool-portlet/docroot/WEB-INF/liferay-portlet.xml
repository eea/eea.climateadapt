		<header-portlet-javascript>http://serverapi.arcgisonline.com/jsapi/arcgis/3.4compact</header-portlet-javascript>
		<header-portlet-javascript>/js/ext-base.js</header-portlet-javascript>
		<header-portlet-javascript>/js/ext-all.js</header-portlet-javascript>
